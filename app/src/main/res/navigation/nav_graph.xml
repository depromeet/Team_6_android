<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.teumteum.teumteum.presentation.home.HomeFragment"
        android:label="HomeFragment" >

        <action
            android:id="@+id/action_homeFragment_to_moimFragment"
            app:destination="@id/moimFragment" />

        <action
            android:id="@+id/action_homeFragment_to_MyPageFragment"
            app:destination="@id/myPageFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_homeFragment_to_teumteumFragment"
            app:destination="@id/teumTeumFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true" />

    </fragment>


    <fragment
        android:id="@+id/moimFragment"
        android:name="com.teumteum.teumteum.presentation.moim.MoimFragment"
        android:label="MoimFragment" >
        <action
            android:id="@+id/action_moimFragment_to_webViewFragment"
            app:destination="@id/webviewFragment" />

        <action
            android:id="@+id/action_moimFragment_to_homeFragment"
            app:destination="@id/homeFragment" />


    </fragment>

    <fragment
        android:id="@+id/webviewFragment"
        android:name="com.teumteum.teumteum.WebviewFragment"
        android:label="WebviewFragment">
        <action
            android:id="@+id/action_webViewFragment_to_moimFragment"
            app:destination="@id/moimFragment" />
    </fragment>

    <fragment
        android:id="@+id/teumTeumFragment"
        android:name="com.teumteum.teumteum.presentation.teumteum.TeumTeumFragment"
        android:label="TeumTeumFragment" />


    <fragment
        android:id="@+id/myPageFragment"
        android:name="com.teumteum.teumteum.presentation.mypage.MyPageFragment"
        android:label="MyPageFragment" />

</navigation>